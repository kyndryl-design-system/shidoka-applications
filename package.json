    "eslint": "^8.31.0",
    "eslint-plugin-storybook": "^0.6.15",
    "extract-loader": "^5.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "lit-analyzer": "^1.2.1",
