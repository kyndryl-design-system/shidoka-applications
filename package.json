{
  "name": "@kyndryl-design-system/shidoka-applications",
  "version": "0.0.0-semantic-release",
  "description": "Shidoka Web Components for Applications",
  "license": "MIT",
  "main": "index.js",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kyndryl-design-system/shidoka-applications.git"
  },
  "scripts": {
    "dev": "npm run storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build": "rollup -c && npm run sass-build",
    "sass-utility": "sass --no-source-map --style=compressed src/common/scss/utility:dist/common/css",
    "sass-build": "npm run sass-utility",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint . --ext .js,.ts",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "analyze": "cem analyze --litelement --globs \"src/components/**/*.ts\" --exclude \"src/**/(*.stories|*.sample).ts\"",
    "test": "vitest run --project=storybook",
    "prepare": "npx husky install"
  },
  "dependencies": {
    "@kyndryl-design-system/shidoka-foundation": "^2.7.1",
    "@kyndryl-design-system/shidoka-icons": "^2.14.0",
    "@lit/context": "^1.1.0",
    "deepmerge-ts": "^7.1.0",
    "flatpickr": "^4.6.13",
    "gridstack": "^12.2.1",
    "lit": "^3.3.0",
    "lottie-web": "^5.12.2",
    "prismjs": "^1.29.0",
    "query-selector-shadow-dom": "^1.0.1",
    "shadow-dom-testing-library": "^1.12.0",
    "swiper": "^11.1.12"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@custom-elements-manifest/analyzer": "^0.9.4",
    "@floating-ui/dom": "^1.7.1",
    "@kyndryl-design-system/shidoka-charts": "^2.5.3",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.3",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-a11y": "^9.0.13",
    "@storybook/addon-designs": "^10.0.1",
    "@storybook/addon-docs": "^9.0.13",
    "@storybook/addon-links": "^9.0.13",
    "@storybook/addon-themes": "^9.0.13",
    "@storybook/addon-vitest": "^9.0.13",
    "@storybook/web-components-vite": "^9.0.13",
    "@types/prismjs": "^1.26.4",
    "@types/query-selector-shadow-dom": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "axe-playwright": "^2.2.2",
    "chromatic": "^11.2.0",
    "eslint": "^8.57.0",
    "eslint-plugin-lit-a11y": "^1.1.0-next.1",
    "eslint-plugin-storybook": "^9.0.13",
    "husky": "^8.0.3",
    "lint-staged": "^15.4.3",
    "lit-analyzer": "^2.0.3",
    "playwright": "^1.56.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.2",
    "prismjs-components-importer": "^0.2.0",
    "remark-gfm": "^4.0.0",
    "resolve-url-loader": "^5.0.0",
    "rollup": "^4.42.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-inline-svg": "^3.0.3",
    "rollup-plugin-multi-input": "^1.5.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-postcss-lit": "^2.2.0",
    "sass": "^1.89.2",
    "sass-embedded": "^1.89.2",
    "storybook": "^9.0.13",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "customElements": "custom-elements.json",
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,ts,json,css,scss,md,mdx}": "prettier --write"
  }
}
