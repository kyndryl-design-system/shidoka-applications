    "lit-css-loader": "^3.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.2",
    "prismjs-components-importer": "^0.2.0",
    "remark-gfm": "^4.0.0",
    "resolve-url-loader": "^5.0.0",
