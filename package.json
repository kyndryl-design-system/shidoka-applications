{
  "name": "@kyndryl-design-system/shidoka-applications",
  "version": "0.0.0-semantic-release",
  "description": "Shidoka Web Components for Applications",
  "main": "index.js",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kyndryl-design-system/shidoka-applications.git"
  },
  "scripts": {
    "dev": "npm run storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy-storybook": "storybook-to-ghpages",
    "build": "rollup -c",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint . --ext .js,.ts",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "analyze": "cem analyze --litelement --globs \"src/**/*.ts\" --exclude \"src/**/*.stories.ts\"",
    "test": "wtr",
    "test:coverage": "wtr --coverage",
    "prepare": "npx husky install"
  },
  "dependencies": {
    "@carbon/icon-helpers": "^10.37.0",
    "@carbon/icons": "^11.14.0",
    "lit": "^3.1.2"
  },
  "peerDependencies": {
    "@kyndryl-design-system/shidoka-foundation": "^1.3.22"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-proposal-decorators": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@custom-elements-manifest/analyzer": "^0.6.8",
    "@kyndryl-design-system/shidoka-charts": "^1.1.5",
    "@kyndryl-design-system/shidoka-foundation": "^1.3.22",
    "@open-wc/testing": "^3.1.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.0.0",
    "@storybook/addon-actions": "8.0.0",
    "@storybook/addon-docs": "8.0.0",
    "@storybook/addon-essentials": "8.0.0",
    "@storybook/addon-links": "8.0.0",
    "@storybook/addon-storysource": "8.0.0",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
    "@storybook/addons": "7.6.17",
    "@storybook/storybook-deployer": "^2.8.16",
    "@storybook/theming": "8.0.0",
    "@storybook/web-components": "8.0.0",
    "@storybook/web-components-webpack5": "8.0.0",
    "@types/carbon__icon-helpers": "^10.7.2",
    "@types/query-selector-shadow-dom": "^1.0.1",
